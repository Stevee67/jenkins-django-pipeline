node('docker') {
    stage 'Checkout'
        checkout scm
    stage 'Build & UnitTest'
        sh "echo 'HHEHHEH'"
        sh "ls -l"
        sh "cd app"
        sh "ls -l"
        sh "docker build -t conduict:B${BUILD_NUMBER} -f Dockerfile ."
        sh "docker-compose run --rm conduict bash -c 'python manage.py test'"
}